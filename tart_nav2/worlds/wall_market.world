
<sdf version='1.6'>
  <world name='default'>
    <!-- Light source -->
    <include>
      <uri>model://sun</uri>
    </include>
    
    <!-- Ground Plane -->
    <include>
      <uri>model://ground_plane</uri>
    </include>
    
    <!-- Wall 1 -->
    <model name='wall_1'>
      <static>true</static>
      <pose>4 0 1 0 0 0</pose> <!-- Adjusted to place the center of the wall -->
      <link name='link'>
        <collision name='collision'>
          <geometry>
            <box>
              <size>0.2 6 2</size> <!-- Thickness, Length, Height -->
            </box>
          </geometry>
        </collision>
        <visual name='visual'>
          <geometry>
            <box>
              <size>0.2 6 2</size>
            </box>
          </geometry>
          <material>
            <script>
              <uri>file://media/materials/scripts/gazebo.material</uri>
              <name>Gazebo/DarkGrey</name>
            </script>
          </material>
        </visual>
      </link>
    </model>
    
